{
    "configurations": [
        {
            "name": "C/C++: g++-11 CDA WalkSAT",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/Debug/CDA_WalkSAT.out",
            "args": ["100", "240", "3", "1", "0.01", "20.0", "1e-5", "6"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}/Debug",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: g++-11 CDA WalkSAT",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
